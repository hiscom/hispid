<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../accessories/style/human.xsl"?>
<thesaurus xmlns="http://rs.gbif.org/thesaurus/"
		   dc:URI="http://hiscom.chah.org.au/hispid/vocabulary/association_type"
		   dc:title="HISPID Association Type Vocabulary"
		   xmlns:dc="http://purl.org/dc/terms/"
		   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		   xsi:schemaLocation="http://rs.gbif.org/thesaurus/  http://rs.gbif.org/schema/thesaurus.xsd" dc:issued="2015-10-23">
	<concept dc:identifier="host"
			 dc:URI="http://hiscom.chah.org.au/hispid/vocabulary/association_type/host"
			 dc:description="the organism with which a parasite or commensal organism lives" dc:issued="2015-10-23">
					<preferred>
						<term dc:title="host"></term>
					</preferred>
				</concept>
	<concept dc:identifier="growingWith"
			 dc:URI="http://hiscom.chah.org.au/hispid/vocabulary/association_type/growingWith"
			 dc:description="other species that were observed to be growing with the specimen at the time of collection" dc:issued="2015-10-23">
					<preferred>
						<term dc:title="growingWith"></term>
					</preferred>
				</concept>
	<concept dc:identifier="growingOn"
			 dc:URI="http://hiscom.chah.org.au/hispid/vocabulary/association_type/growingWith"
			 dc:description="the organism upon which the specimen was growing at time of collection, particularly for non-parasitic taxa (e.g., epiphytes)" dc:issued="2015-10-23">
					<preferred>
						<term dc:title="growingOn"></term>
					</preferred>
				</concept>
	<concept dc:identifier="mixedWith"
			 dc:URI="http://hiscom.chah.org.au/hispid/vocabulary/association_type/mixedWith"
			 dc:description="other taxa that were collected as part of the same gathering, and form part of the same specimen (esp. cryptogams)" dc:issued="2015-10-23">
					<preferred>
						<term dc:title="mixedWith"></term>
					</preferred>
				</concept>
</thesaurus>
